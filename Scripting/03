if [ $# -eq 0 ] || [ $# -ge 2 ]
then
	echo "$0 usage: <user>"
	exit 0
fi

#Check if the user is already log
#Return : 
# 0 - User not log
# 1 - User log
check_log()
{
	USER_LIST=($(who | cut -d " " -f 1))
	for user in ${USER_LIST[*]}
	do
		[ "$1" = "$user" ] && return 1
	done
	return 0
}

check_log $1
if [[ $? == 1 ]]
then
	echo "Del $1"
	userdel -fr $1
else
	echo "No deletion"
fi
